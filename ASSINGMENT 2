#include<stdio.h>
int main()
{
   int n,i,temp,j;
   printf("enter the size of the array");
   scanf("%d",&n);
   int arr[n];
   printf("enter %d elements:",n);
   for(i=0;i<n;i++)
   {
   scanf("%d",&arr[i]);
   }
   for(i=0;i<n;i++)
   {
   for(j=0;j<n-i;j++)
   {
   if(arr[j]>arr[j+1])
   {
   temp=arr[j];
   arr[j]= arr[j+1];
   arr[j+1]= temp;
   }
   }
   }
   printf("sorted array:");
   for(i=0;i<n;i++)
   {
     printf("%d" ,arr[i]);
     }
     return 0;
     }
