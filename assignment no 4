#include <stdio.h>

int main() {
    int n, i, j, temp;

    printf("Enter Number Of Elements: ");
    scanf("%d", &n);
    int a[n];

    printf("Enter Array Elements: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    
    for (i = 1; i < n; i++) {
        temp= a[i];  
        for (j = 0; j < i; j++) {
            if (a[j] > temp) {
                
                for (int k = i; k > j; k--) {
                    a[k] = a[k - 1];
                }
                a[j] = temp;  
                break;  
            }
        }
    }

    printf("Sorted Array Is: ");
    for (i = 0; i < n; i++) {
        printf("%d\t", a[i]);
    }

    return 0;
}



